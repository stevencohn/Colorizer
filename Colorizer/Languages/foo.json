{
  "name": "Foo",
  "rules": [
    {
      "pattern": "(//.*?)\\r?$",
      "captures": [
        "Comment"
      ]
    },
    {
      "pattern": "\\b(foo|bar)\\b",
      "captures": [
        "Keyword"
      ]
    },
    {
      "pattern": "\\b([0-9]{1,})\\b",
      "captures": [
        "Number"
      ]
    },
    {
      "pattern": "\\b((a|b|c)([+-*\\\\])(x|y|z)\\b",
      "captures": [
        "Nested",
        "Operand1",
        "Operator",
        "Operand2"
      ]
    }
  ]
}
