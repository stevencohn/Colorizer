{
  "name": "Python",
  "rules": [
    {
      "pattern": "^[\\s]*(\\\"{3}[^\\r\\n]*)$",
      "scope": "Comment",
      "captures": [
        "Comment"
      ]
    },
    {
      "pattern": "^([^\\r\\n]*\\\"{3})[\\s]*$",
      "scope": "",
      "captures": [
        "Comment"
      ]
    },
    {
      "pattern": "(#.*?)\\r?$",
      "captures": [
        "Comment"
      ]
    },
    {
      "pattern": "('[^\\n]*?(?<!\\\\)')",
      "captures": [
        "String"
      ]
    },
    {
      "pattern": "([^\"]\"[^\\n]*?(?<!\\\\)\")",
      "captures": [
        "String"
      ]
    },
    {
      "pattern": "\\b(and|as|assert|async|await|break|class|continue|def|del|elif|else|except|exec|False|finally|for|from|global|if|import|in|is|lambda|None|not|or|pass|print|raise|return|True|try|while|yield)\\b",
      "captures": [
        "Keyword"
      ]
    },
    {
      "pattern": "\\b([0-9]{1,})\\b",
      "captures": [
        "Number"
      ]
    }
  ]
}
